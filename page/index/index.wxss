.calendar-container{
  text-align: center;
  align-items: center;
  justify-content: center;
}
.calender-time{
  width: 100%;
  line-height: 150rpx;
  color: #0078A8;
}
.calender-prev{
  width: 100%;
  line-height: 200rpx;
}
.calender-btn{
  width: 300rpx;
  height: 300rpx;
  border-radius: 50%;
  font-size: 40rpx;
  line-height: 300rpx;
  color: #fff;
  background-color: #0078A8;
}
.button-hover {
  transform: scale(0.8)
}
.contact{
  width: 80rpx;
  height: 80rpx;
  position: fixed;
  bottom: 100rpx;
  right: 10rpx;
  background-size: 100%;
  border: none;
  border-radius: 100%;
  background-image: url(data:image/png;base64,);
}