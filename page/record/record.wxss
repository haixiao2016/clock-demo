.record-title{
  font-size: 36rpx;
  text-align: center;
  line-height: 100rpx;
  background-color: #1296db;
  color: #fff;
}
.item-title{
  font-size: 32rpx;
  line-height: 80rpx;
  padding: 0 20rpx 0 20rpx;
  border-bottom:1px solid #e5e5e5;
}
.record-item{
  border-bottom:4px solid #e5e5e5;
}
.item-des{
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.des-caMo,.des-caAf{
  flex: 1;
  font-size: 26rpx;
  text-align: center;
}
.del{
  width:400rpx;
  height:80rpx;
  background-color: #1296db;
  color: #fff;
  line-height: 80rpx;
  margin-top: 40rpx;
}
.record-none{
  font-size: 32rpx;
  line-height: 80rpx;
  text-align: center;
}
.title-right{
  float: right;
}
.week{
  color:#1296db;
}
.low{
  background-color: #1296db;
  padding:4rpx 14rpx;
  border-radius: 8rpx;
  margin-left: 10rpx;
  color: #fff;
}